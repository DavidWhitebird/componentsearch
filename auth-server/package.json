{
  "author": "Spotify",
  "name": "server",
  "description": "Server based on Spotify examples of authorization flows through OAuth 2",
  "version": "0.0.3",
  "repository": {},
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.16.0",
    "cookie-parser": "1.3.2",
    "cors": "2.8.4",
    "express": "4.16.0",
    "querystring": "0.2.0",
    "request": "2.83.0"
  },
  "devDependencies": {
    "babel-register": "^6.22.0",
